meme_dict = {
    "КРИНЖ": "Что-то очень странное или стыдное",
    "ЛОЛ": "Что-то очень смешное",
    "РОФЛ": "Шутка",
    "АГРИТЬСЯ": "Злиться",
    "ХП": "Очки здоровья"
}
while True:
    word = input("Укажите слово для объяснения")
    if word in meme_dict.keys():
        print(meme_dict[word])
    else:
        print("Упс, такого слова не нашлось")
    number = input("Хотите продолжить? 1=да, 2=нет")
    if number == 2:
        break
